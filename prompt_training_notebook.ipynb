# دالة تقييم البرومبت
def evaluate_prompt(scores):
    """
    تقييم البرومبت بناءً على المعايير المحددة
    scores: قاموس يحتوي على درجات كل معيار من 1-10
    """
    weights = {
        "الوضوح": 0.25,
        "الاكتمال": 0.20,
        "الدقة": 0.25,
        "الإبداع": 0.15,
        "القابلية للتطبيق": 0.15
    }
    
    total_score = sum(scores[criterion] * weights[criterion] for criterion in scores)
    
    if total_score >= 8.5:
        grade = "ممتاز 🌟"
    elif total_score >= 7.5:
        grade = "جيد جداً 👍"
    elif total_score >= 6.5:
        grade = "جيد ✅"
    elif total_score >= 5.5:
        grade = "مقبول ⚠"
    else:
        grade = "يحتاج تحسين 🔧"
    
    return total_score, grade

